<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
        <servlet-name>ScsCarrierDataFeed</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.ScsCarrierDataFeed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScsCarrierDataFeed</servlet-name>
        <url-pattern>/ScsCarrierDataFeed</url-pattern>
    </servlet-mapping>
    
  <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    
  <servlet>
        <servlet-name>ScsUpdateStatusServlet</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.ScsUpdateStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScsUpdateStatusServlet</servlet-name>
        <url-pattern>/ScsUpdateStatusServlet</url-pattern>
    </servlet-mapping>
    
  	<servlet>
        <servlet-name>ScsPickupJobServlet</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.ScsPickupJobServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScsPickupJobServlet</servlet-name>
        <url-pattern>/ScsPickupJobServlet</url-pattern>
    </servlet-mapping>
    
  <servlet>
        <servlet-name>ScsNewJobTicket</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.ScsNewJobTicket</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScsNewJobTicket</servlet-name>
        <url-pattern>/ScsNewJobTicket</url-pattern>
    </servlet-mapping>
    
  <servlet>
        <servlet-name>ScsGetPickingStatusServlet</servlet-name>
        <servlet-class>cn.com.axtg.scs.feed.ScsGetPickingStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScsGetPickingStatusServlet</servlet-name>
        <url-pattern>/ScsGetPickingStatusServlet</url-pattern>
    </servlet-mapping>
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
